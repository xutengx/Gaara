{
    "name": "xutengx/gaara",
    "description": "Framework For PHPer.",
    "keywords": ["framework", "xuteng", "gaara"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "xuteng",
            "email": "l771033392@qq.com"
        }
    ],
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "App/",
            "Main\\": "Main/"
        },
        "classmap":[
        ],
         "files": [
             "Main/Quick/Function.php",
             "Main/Quick/ClassAilas.php"
        ]
    },
    "require": {
        "php": ">=7.1.0",
        "phpmailer/phpmailer": "^5.2",
        "ezyang/htmlpurifier": "^4.9",
        "workerman/workerman": "^3.4",
        "monolog/monolog": "^1.23",
        "php-console/php-console": "^3.1"
    },
    "scripts": {
        "post-install-cmd": [
            "php -r \"file_exists('env.php') || copy('env.example.php', 'env.php');\""
        ]
    },
    "require-dev": {
        "eaglewu/swoole-ide-helper": "dev-master"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
